{
	"title"      :"add journal statuses",
	"description":"add journal statuses data for send to server only IDs",
	"type"       :"object",
	"properties" :{
		"key" :{
			"type"       :"string",
			"pattern"    :"EMS_Status_pref",
			"description":"Statuses of the user.",
			"maxLength"  :15,
			"minLength"  :15
		},
		"data":{
			"type"       :"array",
			"uniqueItems":true,
			"description":"statuses of user",
			"items"      :{
				"type"       :"object",
				"description":"status of user",
				"properties" :{
					"id"  :{
						"type"       :"number",
						"description":"status ID"
					},
					"name":{
						"type"       :"string",
						"description":"status name",
						"maxLength"  :64,
						"minLength"  :3,
						"pattern"    :"[A-Za-z0-9_-]+"
					}
				},
				"required"   :[
					"id",
					"name"
				]
			}
		}
	},
	"required"   :[
		"key",
		"data"
	]
}
