{
	"title"      :"get all preferences",
	"description":"get all local preferences by user ID",
	"type"       :"object",
	"properties" :{
		"key" :{
			"type"       :"string",
			"pattern"    :"EMS_User_pref",
			"description":"Name key of the user.",
			"maxLength"  :13,
			"minLength"  :13
		},
		"data":{
			"type"       :"object",
			"description":"all preferences",
			"properties" :{
				"timestamp"       :{
					"type"       :"number",
					"description":"timestamp create preferences",
					"minimum"    :0
				},
				"mode"            :{
					"type"       :"string",
					"description":"mode name.",
					"maxLength"  :5,
					"minLength"  :4,
					"pattern"    :"dark|light"
				},
				"darkLightChecked":{
					"type"       :"boolean",
					"description":"mode check/uncheck."
				},
				"leftRightChecked":{
					"type"       :"boolean",
					"description":"side menu left/right."
				},
				"nav_side"        :{
					"type"       :"object",
					"description":"all preferences",
					"properties" :{
						"left" :{
							"type"       :"string",
							"description":"left menu window key.",
							"maxLength"  :8,
							"minLength"  :4,
							"pattern"    :"users|settings"
						},
						"right":{
							"type"       :"string",
							"description":"right menu window key.",
							"maxLength"  :8,
							"minLength"  :5,
							"pattern"    :"users|settings"
						}
					},
					"required"   :[
						"left",
						"right"
					]
				},
				"user"            :{
					"type"       :"object",
					"description":"user preferences",
					"properties" :{
						"id"   :{
							"anyOf":[
								{
									"type"       :"string",
									"pattern"    :"[0-9a-fA-F]{8}\\-[0-9a-fA-F]{4}\\-[0-9a-fA-F]{4}\\-[0-9a-fA-F]{4}\\-[0-9a-fA-F]{12}",
									"description":"id of the user.",
									"maxLength"  :36,
									"minLength"  :36
								},
								{
									"type"       :"string",
									"pattern"    :"Guest",
									"description":"guest user.",
									"maxLength"  :5,
									"minLength"  :5
								}
							]
						},
						"guest":{
							"type"       :"string",
							"description":"Name of guest user.",
							"maxLength"  :5,
							"minLength"  :5,
							"pattern"    :"Guest"
						},
						"username":{
							"type"       :"string",
							"description":"Username of user.",
							"maxLength"  :64,
							"minLength"  :6,
							"pattern"    :"[A-Za-z0-9_-]+"
						}
					},
					"required"   :[
						"id",
						"guest",
						"username"
					]
				}
			},
			"required"   :[
				"mode",
				"darkLightChecked",
				"leftRightChecked",
				"nav_side",
				"user"
			]
		}
	},
	"required"   :[
		"key"
	]
}
